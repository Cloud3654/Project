#include <iostream>
#include <algorithm>
#define NOMINMAX
#include <windows.h>
using namespace std;

int p1card[8];
int p2card[8];
int tmp[8];

int main()
{
    cout << "P1: Input Card Number (8 in 1 ~ 16, Ascending)" << endl;
    while (true) {
        for (int i = 0; i < 8; i++) cin >> p1card[i];
        tmp = p1card;
        sort(tmp);
        if (p1card != tmp) { cout << "* Sort in Ascending Order!" << endl; continue; }
        if (min_element())
        break;
    }
    system("cls");
}
